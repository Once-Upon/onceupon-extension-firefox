{
  "manifest_version": 2,
  "name": "Once Upon Redirect",
  "version": "0.1",

  "description": "Redirect from Etherscan to Once Upon (onceupon.gg)",

  "icons": {
    "48": "icons/ou48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://etherscan.io/*"],
      "js": ["redirect.js"]
    }
  ]
}
